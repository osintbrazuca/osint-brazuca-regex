        },
        {
            "regex": "^\\d{8}\\-?\\d{1}$",
            "description": "SE"
        },
        {
            "regex": "^(29)(01|02|03|99)\\d{7}$",
            "description": "TO"
        }
    ],
    "email": [
        {
            "regex": "([\\w._%+-]+)(@|\\s@\\s|\\sat\\s|\\[at\\])([\\w.-]+)\\.([\\w]{2,})",
            "description": "Endereço de email"
        }
    ],
    "hora12": [
        {
            "regex": "((0?[1-9]|1[0-2]):([0-5][0-9].?([a].?[m].?|[p].?[m].?)))",
            "description": "Hora no formato 12h"
        }
    ],
    "hora24": [
        {
            "regex": "([01][0-9]|[2][0-3]):([0-5][0-9])",
            "description": "Hora no formato 24h"
        }
    ],
    "ip": [
        {
            "regex": "[[:digit:]]{1,3}\\.[[:digit:]]{1,3}\\.[[:digit:]]{1,3}\\.[[:digit:]]{1,3}",
            "description": "IP - Internet Protocol"
        }
    ],
    "ipv6": [
        {
            "regex": "(([0-9a-fA-F]{1,4}:){7,7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:)|fe80:(:[0-9a-fA-F]{0,4}){0,4}%[0-9a-zA-Z]{1,}|::(ffff(:0{1,4}){0,1}:){0,1}((25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])\\.){3,3}(25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])|([0-9a-fA-F]{1,4}:){1,4}:((25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])\\.){3,3}(25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9]))",
            "description": "IPv6 - Internet Protocol versão 6"
        }
    ],
    "macAddress": [
        {
            "regex": "(?:[0-9A-Fa-f]{2}[:-]){5}(?:[0-9A-Fa-f]{2})",
            "description": "Media Access Control address"
        }
    ],
    "passaporte": [
        {
            "regex": "^[A-Z]{2}\\d{6}$",
            "description": "Número do Passaporte"
        }
    ],
    "pixChave": [
        {
            "regex": "([0-9]{14,20})([bB][rR]\\.[gG][oO][vV]\\.[bB][cC][bB]\\.[pP][iI][xX]).*(6304)([0-9a-zA-Z]{4})",
            "description": "Chave PIX"
        }
    ],
    "pixChaveRandom": [
        {
            "regex": "([a-z\\d]{8})\\-([a-z\\d]{4})\\-([a-z\\d]{4})\\-([a-z\\d]{4})\\-([a-z\\d]{12})",
            "description": "Chave PIX Aleatória"
        }
    ],
    "placaDetran": [
        {
            "regex": "[A-Z]{3}\\d[0-9A-Z]\\d{2}",
            "description": "Placas de Carros e Motos Brasileiras (incluindo Mercosul)"
        }
    ],
    "renavam": [
        {
            "regex": "((\\d{4})[.](\\d{6})-(\\d{1})|(\\d{4})(\\d{6})(\\d{1}))",
            "description": "Registro Nacional de Veículos Automotores"
        }
    ],
    "rg": [
        {
            "regex": "(\\d{1,2}\\.?)(\\d{3}\\.?)(\\d{3})(\\-?[0-9Xx]{1})",
            "description": "Registro Geral"
        }
    ],
    "rne": [
        {
            "regex": "(RNE)([A-Z\\d])(\\d{6})([A-Z\\d])",
            "description": "Registro Nacional de Estrangeiro"
        }
    ],
    "siglaUF": [
        {
            "regex": "(AC|AL|AP|AM|BA|CE|DF|ES|GO|MA|MT|MS|MG|PA|PB|PR|PE|PI|RJ|RN|RS|RO|RR|SC|SP|SE|TO|BR)",
            "description": "Siglas das Unidades Federativas do Brasil"
        }
    ],
    "telefone": [
        {
            "regex": "(?:(?:(\\+|00)?(55))\\s?)?(?:\\(?(\\d{2})\\)?\\s?)(|\\d{2})(|-)?(?:(9\\d|[2-9])\\d{3}[-|.|\\s]?(\\d{4}))",
            "description": "Telefone Brasil"
        }
    ],
    "url": [
        {
            "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()!@:%_\\+.~#?&\\/\\/=]*)",
            "description": "URL - Uniform Resource Locator"
        }
    ],
    "UUID": [
        {
            "regex": "^[a-fA-F0-9]{8}-[a-fA-F0-9]{4}-4[a-fA-F0-9]{3}-[8|9|aA|bB][a-fA-F0-9]{3}-[a-fA-F0-9]{12}$",
            "description": "UUID - Universally Unique Identifiers"
        }
    ],
    "verificacaoDeSenha": [ 
        { 
            "regex": "(?=.*[A-Z])(?=.*[a-z])(?=.*[\\d])(?=.*[@#$%&*!-+&*]).{8,20}",
            "description": "Validador de senhas com tamanho entre 8 e 20 dígitos e com letras maiúsculas, minúsculas, números e caracteres especiais"
        } 
    ],
    "processoCnj": [ 
        { 
            "regex": "[0-9]{7}\\-?[0-9]{2}\\.?[0-9]{4}\\.?[4-8]\\.?[0-9]{2}\\.?[0-9]{4}",
            "description": "Processos judiciais com numeração padrão do CNJ, em estrutura formatada (com caracteres especiais) ou não"
        } 
    ],
    "numeroDeEndereco": [ 
        { 
            "regex": "^(?:s\\/n|S\\/n|S\\/N|s\\/N)|^(\\d)*$",
            "description": "Número de Endereço (números ou 'S/N', 's/n', 'S/n', 's/N')"
        } 
    ]
}
