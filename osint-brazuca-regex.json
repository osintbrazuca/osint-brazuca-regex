        },
        {
            "regex": "^(03)\\d{7}$",
            "description": "AP"
        },
        {
            "regex": "^\\d{2}.?\\d{3}.?\\d{3}\\-?\\d{1}$",
            "description": "AM, GO"
        },
        {
            "regex": "^\\d{7}\\-?\\d{2}$",
            "description": "BA (9 dígitos), PE"
        },
        {
            "regex": "^\\d{6}\\-?\\d{2}$",
            "description": "BA (8 dígitos)"
        },
        {
            "regex": "^\\d{8}\\-?\\d{1}$",
