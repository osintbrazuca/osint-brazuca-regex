        },
        {
            "regex": "^\\d{10}\\-?\\d{1}$",
            "description": "MT"
        },
        {
            "regex": "^(28)\\d{7}$",
            "description": "MS"
        },
        {
            "regex": "^\\d{3}.?\\d{3}.?\\d{3}\/?\\d{4}$",
            "description": "MG"
        },
        {
            "regex": "^(15)\\d{6}\\-?\\d{1}$",
